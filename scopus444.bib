@ARTICLE{Zhang2024,
	author = {Zhang, Nianbin and Wang, Yunjia and Zhao, Feng and Wang, Teng and Zhang, Kewei and Fan, Hongdong and Zhou, Dawei and Zhang, Leixin and Yan, Shiyong and Diao, Xinpeng and Song, Rui},
	title = {Monitoring and Analysis of the Collapse at Xinjing Open-Pit Mine, Inner Mongolia, China, Using Multi-Source Remote Sensing},
	year = {2024},
	journal = {Remote Sensing},
	volume = {16},
