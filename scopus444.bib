	doi = {10.1007/s12665-014-3806-4},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84940004159&doi=10.1007%2fs12665-014-3806-4&partnerID=40&md5=592f9e60a7422eac4e5698d2f56c7574},
