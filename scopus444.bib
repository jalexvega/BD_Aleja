	correspondence_address = {J. Wu; Institute of Geomechanics, Chinese Academy of Geological Sciences, Beijing, 100081, China; email: wujihuan97@163.com; C. Zhang; Institute of Geomechanics, Chinese Academy of Geological Sciences, Beijing, 100081, China; email: zhangcs401@sina.com},
