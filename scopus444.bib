@ARTICLE{Comerci20152997,
	author = {Comerci, Valerio and Vittori, Eutizio and Cipolloni, Carlo and Di Manna, Pio and Guerrieri, Luca and Nisio, Stefania and Succhiarelli, Claudio and Ciuffreda, Maria and Bertoletti, Erika},
	title = {Geohazards Monitoring in Roma from InSAR and In Situ Data: Outcomes of the PanGeo Project},
	year = {2015},
	journal = {Pure and Applied Geophysics},
	volume = {172},
	number = {11},
