	correspondence_address = {Y. Gül; Civil Engineering, Faculty of Engineering, Sivas Cumhuriyet University, Sivas, 58140, Turkey; email: ygul@cumhuriyet.edu.tr},
	publisher = {Elsevier Ltd},
