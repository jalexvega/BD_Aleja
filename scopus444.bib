@ARTICLE{Lin2024,
	author = {Lin, Kaixiong and Jiapaer, Guli and Yu, Tao and Zhang, Liancheng and Liang, Hongwu and Chen, Bojian and Ju, Tongwei},
	title = {Identification of Potential Landslides in the Gaizi Valley Section of the Karakorum Highway Coupled with TS-InSAR and Landslide Susceptibility Analysis},
