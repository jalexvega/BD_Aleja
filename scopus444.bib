@ARTICLE{Ducrocq2021,
	author = {Ducrocq, Cécile and Geirsson, Halldór and Árnadóttir, Thóra and Juncu, Daniel and Drouin, Vincent and Gunnarsson, Gunnar and Kristjánsson, Bjarni R. and Sigmundsson, Freysteinn and Hreinsdóttir, Sigrún and Tómasdóttir, Sigrún and Blanck, Hanna},
	title = {Inflation-Deflation Episodes in the Hengill and Hrómundartindur Volcanic Complexes, SW Iceland},
	year = {2021},
	journal = {Frontiers in Earth Science},
	volume = {9},
