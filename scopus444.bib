@ARTICLE{Zhao2023,
	author = {Zhao, Chaoying and Chen, Liquan and Yin, Yueping and Liu, Xiaojie and Li, Bin and Ren, Chaofeng and Liu, Donglie},
	title = {Failure process and three-dimensional motions of mining-induced Jianshanying landslide in China observed by optical, LiDAR and SAR datasets},
